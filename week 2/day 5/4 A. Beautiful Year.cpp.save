#include<bits/stdc++.h>
using namespace std;
 int main()
 {
     int year;
     int a,b,c,d;
     cin >> year;
     while(1){
        year++;
        a=year/1000;  cout<<"A "<<a<<endl;
        b=year/100; cout<<"B "<<b<<endl;
        b=b%10; cout<<"Mod B "<<b<<endl;
        c=year/10; cout<<"C "<<c<<endl;
        c=c%10; cout<<"C "<<c<<endl;
        d=year%10;cout
        if(a!=b && a!=c && a!=d && b!=c && b!=d && c!=d)
            break;
     }
     cout<< year << endl;
     return 0;
 }
